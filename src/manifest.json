{
    "name": "Huddle01",
    "description": "Chrome Extension for Huddle01",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "32": "/images/32.png",
            "48": "/images/48.png",
            "128": "/images/128.png" 
        }
    },
    "permissions": ["storage", "activeTab", "scripting", "identity", "identity.email"],
    "icons": {
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    },
    "oauth2": {
        "client_id": "99789589928-7ndhsqanirtccmokb2pe9265a69vbots.apps.googleusercontent.com",
        "scopes": [
            "profile email",
            "https://www.googleapis.com/auth/calendar"
        ]
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }, 
    "key": ""
  }